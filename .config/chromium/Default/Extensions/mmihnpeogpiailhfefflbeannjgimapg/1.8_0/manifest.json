{
   "background": {
      "persistent": false,
      "scripts": [ "js/jquery-2.1.3.min.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/fluff.png"
      },
      "default_popup": "popup.html",
      "default_title": "Fluffify"
   },
   "content_scripts": [ {
      "css": [ "css/content.css" ],
      "js": [ "js/jquery-2.1.3.min.js", "js/content_script.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Corporate phrases turn our stomach. We're replacing the terms we're so sick of hearing - to a fun equivalent. Contribute on GitHub!",
   "key": "",
   "manifest_version": 2,
   "name": "Fluffify for Chrome",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "storage", "*://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8"
}
